Процедура тпВсдВыбор()
	ГМ.ПриАктивизацииСтрокиТП(тпВСД, тзВСД);
	ГМ.ПриАктивизацииКолонкиТП(тпВСД, тзВСД);
	
	текСтр=тзВСД.ТекущаяСтрока();
	Если текСтр=0 Тогда Возврат; КонецЕсли;
	текКол = тзВСД.ТекущаяКолонка();
	
	Эл = тзВСД.ПолучитьЗначение(текСтр, текКол);
	
	Если текКол = "ПроизводителиСписок" Тогда
		Сп = ЗначениеИзСтроки(тзВСД.ПроизводителиСписок);
		зн="";
		Сп.ВыбратьСтроку(зн,"Производители");
	ИначеЕсли текКол = "МаршрутСледования" Тогда
		ИзмМаршрут = УстановитьНомерАвтоВМаршрут(тзВСД.МаршрутСледования);
		Если ПустоеЗначение(ИзмМаршрут) = 0 тогда
			тзВСД.МаршрутСледования = ИзмМаршрут;
		КонецЕсли; 
	ИначеЕсли текКол = "НомерПартии" Тогда
		Сп = ЗначениеИзСтроки(тзВСД.НомерПартии);
		Если ТипЗначенияСтр(Сп) = "СписокЗначений" Тогда
			зн="";
			Сп.ВыбратьЗначение(зн,"№ Партий");
		КонецЕсли;
		тпВСД.ОбновитьСтроки();
	ИначеЕсли текКол = "УровниУпаковки" Тогда
		
		меню = СоздатьОбъект("СписокЗначений");
		меню.ДобавитьЗначение("Открыть","Открыть");
		меню.ДобавитьЗначение("Очистить","Очистить"); 
			
		стр=0; Зн="";
		Если меню.ВыбратьЗначение(Зн, "", стр, 60, 1) = 1 Тогда
							
			Если Зн ="Открыть"  Тогда 
		
				стрУровни = тзВСД.ПолучитьЗначение(текСтр, текКол);
			    КаталогОбработки = ГМ.СписокКонстант.Получить("КаталогМодуля");
			    ОткрытьФорму("Отчет", стрУровни, КаталогОбработки+"ВСД_УровниУпаковки.ert");

			ИначеЕсли Зн ="Очистить" Тогда
				
				тзВСД.УстановитьЗначение(текСтр, текКол,"");				
				
			КонецЕсли;
			
			тпВСД.ОбновитьСтроки();
		КонецЕсли;
		
	ИначеЕсли текКол = "СвязанныеДокументы" Тогда
		Сп = ЗначениеИзСтроки(тзВСД.СвязанныеДокументы);
		Сп.ВыбратьСтроку(,"Документы");
	ИначеЕсли текКол = "Номенклатура" Тогда
		
		меню = СоздатьОбъект("СписокЗначений");
		меню.ДобавитьЗначение("Открыть","Открыть");
		меню.ДобавитьЗначение("Добавить","Добавить");
		меню.ДобавитьЗначение("Изменить","Изменить"); 
		меню.ДобавитьЗначение("Очистить","Очистить"); 
			
		стр=0; Зн="";
		Если меню.ВыбратьЗначение(Зн, "", стр, 60, 1) = 1 Тогда
							
			Если Зн ="Открыть"  Тогда 	
				
				ОткрытьФорму(тзВСД.Номенклатура);
				
			ИначеЕсли Зн ="Добавить"  Тогда

				ОткрытьПодбор("Справочник.Номенклатура","ДляВыбора",,0, тзВСД.Номенклатура);

			ИначеЕсли Зн ="Изменить"  Тогда 	

				Если (ПустоеЗначение(тзВСД.Продукция_Элемент)=0) и (ПустоеЗначение(тзВСД.Номенклатура)=0) Тогда
					ГМ.УдалитьСоответсвие_Номенклатура_Продукция_Элемент( тзВСД.Номенклатура, тзВСД.Продукция_Элемент);
					тзВСД.Номенклатура = "";
				КонецЕсли;
				ОткрытьПодбор("Справочник.Номенклатура","ДляВыбора",,0, тзВСД.Номенклатура);

			ИначеЕсли Зн ="Очистить"  Тогда 	

				Если (ПустоеЗначение(тзВСД.Продукция_Элемент)=0) и (ПустоеЗначение(тзВСД.Номенклатура)=0) Тогда
					ГМ.УдалитьСоответсвие_Номенклатура_Продукция_Элемент( тзВСД.Номенклатура, тзВСД.Продукция_Элемент);
					тзВСД.Номенклатура = "";
				КонецЕсли;
				
			КонецЕсли;
		КонецЕсли;
		
		тпВСД.ОбновитьСтроки();

	ИначеЕсли текКол = "Количество" Тогда
		// Менять нельзя
	ИначеЕсли текКол = "ДатаСрокГодностиТЗ" Тогда
		Сп = ЗначениеИзСтроки(тзВСД.ДатаСрокГодностиТЗ);
		Если ТипЗначенияСтр(Сп) = "ТаблицаЗначений" Тогда
			Сп.ВыбратьСтроку(,"Срок Годности");
		КонецЕсли;
		тпВСД.ОбновитьСтроки();
	ИначеЕсли текКол = "ДатаИзготовленияТЗ" Тогда
		Сп = ЗначениеИзСтроки(тзВСД.ДатаИзготовленияТЗ);
		Если ТипЗначенияСтр(Сп) = "ТаблицаЗначений" Тогда
			Сп.ВыбратьСтроку(,"Изготовлено");
		КонецЕсли;
		тпВСД.ОбновитьСтроки();
	ИначеЕсли текКол = "ДатаИзготовленияТЗ" Тогда
		Сп = ЗначениеИзСтроки(тзВСД.ДатаИзготовленияТЗ);
		Если ТипЗначенияСтр(Сп) = "ТаблицаЗначений" Тогда
			Сп.ВыбратьСтроку(,"Изготовлено");
		КонецЕсли;
		тпВСД.ОбновитьСтроки();
	ИначеЕсли (ТипЗначенияСтр(Эл) = "Справочник") Тогда 

		Эл = тзВСД.ПолучитьЗначение(текСтр, текКол);
		ОткрытьФорму( Эл.ТекущийЭлемент() );
	
	ИначеЕсли (ТипЗначенияСтр(Эл) = "Документ") Тогда
			  	
		Эл = тзВСД.ПолучитьЗначение(текСтр, текКол);
		ОткрытьФорму( Эл.ТекущийДокумент() );
		
	ИначеЕсли (текКол = "КоличествоПринять") или (текКол = "КоличествоВозврат") Тогда
		текЗн = тзВСД.ПолучитьЗначение(ТекСтр, текКол);
		ВвестиЧисло(текЗн, "",15,3);
		тзВСД.УстановитьЗначение(текСтр, текКол, текЗн);		
		тзВСД.Пометка=1;
		тпВСД.ОбновитьСтроки();
	Иначе
		текЗн = тзВСД.ПолучитьЗначение(ТекСтр, текКол);
		ВвестиЗначение(текЗн, "","Строка",100);
		тзВСД.УстановитьЗначение(текСтр, текКол, текЗн);
		тпВСД.ОбновитьСтроки();
	КонецЕсли;
	Если (текКол = "КоличествоПринять") или (текКол = "КоличествоВозврат") Тогда
		//Если (Число(тзВСД.КоличествоПринять)) > (Число(тзВСД.Количество)) Тогда
		//	тзВСД.КоличествоПринять = Число(тзВСД.Количество) ;
		//КонецЕсли;
		//Если (Число(тзВСД.КоличествоВозврат)) > (Число(тзВСД.Количество)) Тогда
		//	тзВСД.КоличествоВозврат = Число(тзВСД.Количество) ;
		//КонецЕсли;
		//Если (Число(тзВСД.КоличествоПринять) + Число(тзВСД.КоличествоВозврат)) > (Число(тзВСД.Количество)) Тогда
		//	тзВСД.КоличествоПринять = Число(тзВСД.Количество) - Число(тзВСД.КоличествоВозврат);
		//КонецЕсли;
	КонецЕсли;
	
КонецПроцедуры

Процедура тпВСДПриВыводеСтроки(ТабличноеПоле,ОформлениеСтроки,ДанныеСтроки,ТипРегиона)

	Если ТипРегиона = 3 Тогда
		ГМ.ВывестиФлажок(ОформлениеСтроки, ДанныеСтроки, "Пометка");
		
		Если (ДанныеСтроки.Количество) <> (ДанныеСтроки.КоличествоПринять) Тогда
			ОбъектЯчейка=ОформлениеСтроки.Ячейки.Получить("Количество");
			ОбъектЯчейка.ЦветФона=ГМ.цвКрасный; 
			ОбъектЯчейка=ОформлениеСтроки.Ячейки.Получить("КоличествоПринять");
			ОбъектЯчейка.ЦветФона=ГМ.цвКрасный; 
		КонецЕсли;

		Если ДанныеСтроки.ПлощадкаИзПЭ <> ДанныеСтроки.ПлощадкаИзВСД Тогда
			ОбъектЯчейка=ОформлениеСтроки.Ячейки.Получить("ПлощадкаИзПЭ");
			ОбъектЯчейка.ЦветФона=ГМ.цвКрасный; 
			ОбъектЯчейка=ОформлениеСтроки.Ячейки.Получить("ПлощадкаИзВСД");
			ОбъектЯчейка.ЦветФона=ГМ.цвКрасный; 
		КонецЕсли;
		
		Если ПустоеЗначение(ДанныеСтроки.ДокВСД)=0 Тогда
			Если СокрЛП(ДанныеСтроки.ДокВСД.Статус)="COMPLETED" Тогда
				ОформлениеСтроки.ЦветФона = ГМ.цвЗеленый;	
			иначеЕсли (ПустоеЗначение(ДанныеСтроки.ДокВСД.Статус)= 1) или (СокрЛП(ДанныеСтроки.ДокВСД.Статус)="CONFIRMED") Тогда
				//тз.сЦвет = "";
			иначе
				ОформлениеСтроки.ЦветФона = ГМ.цвЖелтый;	
			КонецЕсли;
			Если (ПустоеЗначение(ДанныеСтроки.докВСД.Отправитель_ХозСубъект) = 1) или
//	Импорт			(ПустоеЗначение(тз.докВСД.Отправитель_Площадка) = 1) или
				(ПустоеЗначение(ДанныеСтроки.ДокВСД.ПроизводителиСписок) = 1) Тогда
			    ДанныеСтроки.сЦвет = ГМ.цвКрасный;
			КонецЕсли;
		Иначе
			Если _НеЗагружать = 1 Тогда
				Если СокрЛП(ДанныеСтроки.Статус)="COMPLETED" Тогда
					ОформлениеСтроки.ЦветФона = ГМ.цвЗеленый;	
				иначеЕсли (ПустоеЗначение(ДанныеСтроки.Статус)= 1) или (СокрЛП(ДанныеСтроки.Статус)="CONFIRMED") Тогда
					//тз.сЦвет = "";
				иначе
					ОформлениеСтроки.ЦветФона = ГМ.цвЖелтый; 
				КонецЕсли;
				Если (ПустоеЗначение(ДанныеСтроки.Отправитель_ХозСубъект) = 1) или
//Импорт					(ПустоеЗначение(тз.Отправитель_Площадка) = 1) или
					(ПустоеЗначение(ДанныеСтроки.ПроизводителиСписок) = 1) Тогда
					ОформлениеСтроки.ЦветФона = ГМ.цвКрасный;	
				КонецЕсли;
			Иначе
				ОформлениеСтроки.ЦветФона = ГМ.цвКрасный;	
			КонецЕсли;

		КонецЕсли;
	КонецЕсли;
	
	//ДанныеСтроки.КодТНВЭД = ДанныеСтроки.ВидПродукции.КодТНВЭД;
	//ОформлениеСтроки.Ячейки.КодТНВЭД.УстановитьТекст( СокрЛП(ДанныеСтроки.ВидПродукции.КодТНВЭД) );

 	//+Станислав, 2019-06-10 13:59:54
	кодТНВЭД=СокрЛП(ДанныеСтроки.Продукция_Элемент.ВидПродукции.КодТНВЭД)+"/"+СокрЛП(ДанныеСтроки.Продукция_Элемент.Продукция.КодТНВЭД);
		ОформлениеСтроки.Ячейки.КодТНВЭД.УстановитьТекст(кодТНВЭД); 
