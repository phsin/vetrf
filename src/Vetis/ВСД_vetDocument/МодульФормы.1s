						ВСД_Объект.номерКонтейнера 	= тзТочкиМаршрута.номерКонтейнера;
						ВСД_Объект.номеравто			= СокрЛП(тзТочкиМаршрута.номеравто) + СокрЛП(тзТочкиМаршрута.номерВагона) + СокрЛП(тзТочкиМаршрута.НомерАвиарейса)+ СокрЛП(тзТочкиМаршрута.НазваниеСудна);
						ВСД_Объект.номерПолуприцепа	= тзТочкиМаршрута.номерПрицепа;
						Прервать;
					КонецЕсли;
				КонецЦикла;
			КонецЕсли;
			
		Исключение						
		КонецПопытки;
			
	КонецЕсли;

	Попытка
