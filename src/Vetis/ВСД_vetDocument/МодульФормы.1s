		ВСД_Объект.НаименованиеПродукции = ВСД_Объект.Продукция_Элемент.Наименование;
	КонецПопытки;
	ВСД_Объект.Наименование = ВСД_Объект.НаименованиеПродукции;

	//Trade Identification Number (GTIN) - идентификационный номер продукции производителя.
	Попытка ВСД_Объект.GTIN = batch.selectSingleNode("vd:productItem").selectSingleNode("dt:globalID").text;Исключение КонецПопытки;
    //Артикул (код) продукции в соответствии с внутренним кодификатором производителя.
	Попытка ВСД_Объект.Артикул = batch.selectSingleNode("vd:productItem").selectSingleNode("dt:code").text;Исключение КонецПопытки;

	// Фасовка
	Попытка ВСД_Объект.packaging = batch.selectSingleNode("vd:productItem").selectSingleNode("dt:packaging"); Исключение packaging=""	КонецПопытки;
