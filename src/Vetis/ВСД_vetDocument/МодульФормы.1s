   	//		тзРезультат.Производитель_Площадка = СЗПроизводителей.ПолучитьЗначение(1,1);
   	//	КонецЕсли;
   	//Исключение КонецПопытки;

   	//Это в ветке origin - !!!!!
   	//В док-те нет таких реквизитов
