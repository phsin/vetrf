	            Результат = 0;
	        КонецЕсли;			
		КонецЕсли;	
		
	КонецЦикла;

	Если ПустоеЗначение(док.ТермическоеСостояние)=1 Тогда
		Предупреждение("В "+док+" не указано ТермическоеСостояние","!");
		Результат = 0;
	КонецЕсли;

	
	Возврат Результат;
