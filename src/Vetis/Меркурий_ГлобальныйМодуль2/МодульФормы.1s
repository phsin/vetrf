КонецФункции

Функция Получить_Список_Продукции( Площадка ) Экспорт

	ЗапросXML = Получить_Список_Продукции_ЗапросXML( Площадка );

	метод = ГМ.Компонента.GetMethod();

	метод.Service = "platform/services/2.0/ProductService";
