	|<apl:issueDate>"+ ГМ.Преобразовать_Дата_в_Строка(ТекущаяДата(), "T00:00:00") +"</apl:issueDate>
	|<apl:data>
	|<merc:withdrawVetDocumentRequest>
	|<merc:localTransactionId>[GUID]</merc:localTransactionId>
